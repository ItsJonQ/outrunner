  "tokenColors": [
    {
      "name": "Changed",
      "scope": [
        "markup.changed",
        "meta.diff.header.git",
        "meta.diff.header.from-file",
        "meta.diff.header.to-file"
      ],
      "settings": {
        "foreground": "<%= textLight %>",
        "fontStyle": ""
      }
    },
    {
      "name": "Deleted",
      "scope": "markup.deleted.diff",
      "settings": {
        "foreground": "<%= red %>90",
        "fontStyle": ""
      }
    },
    {
      "name": "Inserted",
      "scope": "markup.inserted.diff",
      "settings": {
        "foreground": "<%= green %>ff",
        "fontStyle": ""
      }
    },
    {
      "name": "Global settings",
      "settings": {
        "background": "<%= backgroundPrimary %>",
        "foreground": "<%= white %>"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "<%= textComment %>",
        "fontStyle": ""
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "<%= green %>"
      }
    },
    {
      "name": "String Quoted",
      "scope": ["string.quoted", "variable.other.readwrite.js"],
      "settings": {
        "foreground": "<%= gold %>"
      }
    },
    {
      "name": "Support Constant Math",
      "scope": "support.constant.math",
      "settings": {
        "foreground": "<%= green %>"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric", "constant.character.numeric"],
      "settings": {
        "foreground": "<%= orange %>",
        "fontStyle": ""
      }
    },
    {
      "name": "Built-in constant",
      "scope": [
        "constant.language",
        "punctuation.definition.constant",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "<%= blue %>"
      }
    },
    {
      "name": "User-defined constant",
      "scope": ["constant.character", "constant.other"],
      "settings": {
        "foreground": "<%= blue %>"
      }
    },
    {
      "name": "Constant Character Escape",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "<%= orange %>"
      }
    },
    {
      "name": "RegExp String",
      "scope": ["string.regexp", "string.regexp keyword.other"],
      "settings": {
        "foreground": "<%= blue %>"
      }
    },
    {
      "name": "Comma in functions",
      "scope": "meta.function punctuation.separator.comma",
      "settings": {
        "foreground": "<%= textPrimary %>"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "foreground": "<%= green %>"
      }
    },
    {
      "name": "Keyword",
      "scope": ["punctuation.accessor", "keyword"],
      "settings": {
        "foreground": "<%= brightMagenta %>",
        "fontStyle": ""
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage",
        "meta.var.expr",
        "meta.class meta.method.declaration meta.var.expr storage.type.js",
        "storage.type.property.js",
        "storage.type.property.ts",
        "storage.type.property.tsx"
      ],
      "settings": {
        "foreground": "<%= brightMagenta %>",
        "fontStyle": ""
      }
    },
    {
      "name": "Storage type",
      "scope": "storage.type",
      "settings": {
        "foreground": "<%= blue %>a5"
      }
    },
    {
      "name": "Storage type",
      "scope": "storage.type.function.arrow.js",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "Class name",
      "scope": ["entity.name.class", "meta.class entity.name.type.class"],
      "settings": {
        "foreground": "<%= yellow %>"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "<%= green %>"
      }
    },
    {
      "name": "Function name",
      "scope": "entity.name.function",
      "settings": {
        "foreground": "<%= blue %>",
        "fontStyle": ""
      }
    },
    {
      "name": "Meta Tag",
      "scope": ["punctuation.definition.tag", "meta.tag"],
      "settings": {
        "foreground": "<%= white %>99"
      }
    },
    {
      "name": "HTML Tag names",
      "scope": [
        "entity.name.tag",
        "meta.tag.other.html",
        "meta.tag.other.js",
        "meta.tag.other.tsx",
        "entity.name.tag.tsx",
        "entity.name.tag.js",
        "entity.name.tag",
        "meta.tag.js",
        "meta.tag.tsx",
        "meta.tag.html"
      ],
      "settings": {
        "foreground": "<%= blue %>",
        "fontStyle": ""
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "fontStyle": "",
        "foreground": "<%= cyan %>"
      }
    },
    {
      "name": "Entity Name Tag Custom",
      "scope": "entity.name.tag.custom",
      "settings": {
        "foreground": "<%= cyan %>"
      }
    },
    {
      "name": "Library (function & constant)",
      "scope": ["support.function", "support.constant"],
      "settings": {
        "foreground": "<%= blue %>"
      }
    },
    {
      "name": "Support Constant Property Value meta",
      "scope": "support.constant.meta.property-value",
      "settings": {
        "foreground": "<%= brightCyan %>"
      }
    },
    {
      "name": "Library class/type",
      "scope": ["support.type", "support.class"],
      "settings": {
        "foreground": "<%= teal %>"
      }
    },
    {
      "name": "Support Variable DOM",
      "scope": "support.variable.dom",
      "settings": {
        "foreground": "<%= green %>"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "background": "#ff2c83",
        "foreground": "<%= brightWhite %>"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "<%= brightWhite %>",
        "background": "#d3423e"
      }
    },
    {
      "name": "Keyword Operator",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "<%= brightCyan %>",
        "fontStyle": ""
      }
    },
    {
      "name": "Keyword Operator Relational",
      "scope": "keyword.operator.relational",
      "settings": {
        "foreground": "<%= brightMagenta %>",
        "fontStyle": ""
      }
    },
    {
      "name": "Keyword Operator Assignment",
      "scope": "keyword.operator.assignment",
      "settings": {
        "foreground": "<%= brightMagenta %>"
      }
    },
    {
      "name": "Keyword Operator Arithmetic",
      "scope": "keyword.operator.arithmetic",
      "settings": {
        "foreground": "<%= brightMagenta %>"
      }
    },
    {
      "name": "Keyword Operator Bitwise",
      "scope": "keyword.operator.bitwise",
      "settings": {
        "foreground": "<%= brightMagenta %>"
      }
    },
    {
      "name": "Keyword Operator Increment",
      "scope": "keyword.operator.increment",
      "settings": {
        "foreground": "<%= brightMagenta %>"
      }
    },
    {
      "name": "Keyword Operator Ternary",
      "scope": "keyword.operator.ternary",
      "settings": {
        "foreground": "<%= brightMagenta %>"
      }
    },
    {
      "name": "Double-Slashed Comment",
      "scope": "comment.line.double-slash",
      "settings": {
        "foreground": "<%= textComment %>"
      }
    },
    {
      "name": "Object",
      "scope": "object",
      "settings": {
        "foreground": "<%= brightBlue %>"
      }
    },
    {
      "name": "Null",
      "scope": "constant.language.null",
      "settings": {
        "foreground": "<%= brightRed %>"
      }
    },
    {
      "name": "Meta Brace",
      "scope": "meta.brace",
      "settings": {
        "foreground": "<%= white %>"
      }
    },
    {
      "name": "Meta Delimiter Period",
      "scope": "meta.delimiter.period",
      "settings": {
        "foreground": "<%= brightMagenta %>",
        "fontStyle": ""
      }
    },
    {
      "name": "Punctuation Definition String",
      "scope": "punctuation.definition.string",
      "settings": {
        "foreground": "<%= brightGreen %>"
      }
    },
    {
      "name": "Boolean",
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "<%= brightRed %>"
      }
    },
    {
      "name": "Object Comma",
      "scope": "object.comma",
      "settings": {
        "foreground": "<%= brightWhite %>"
      }
    },
    {
      "name": "Variable Parameter Function",
      "scope": "variable.parameter.function",
      "settings": {
        "foreground": "<%= brightCyan %>",
        "fontStyle": ""
      }
    },
    {
      "name": "Support Type Property Name & entity name tags",
      "scope": [
        "support.type.vendor.property-name",
        "support.constant.vendor.property-value",
        "support.type.property-name",
        "meta.property-list entity.name.tag"
      ],
      "settings": {
        "foreground": "<%= cyan %>",
        "fontStyle": ""
      }
    },
    {
      "name": "Entity Name tag reference in stylesheets",
      "scope": "meta.property-list entity.name.tag.reference",
      "settings": {
        "foreground": "<%= softBlue %>"
      }
    },
    {
      "name": "Constant Other Color RGB Value Punctuation Definition Constant",
      "scope": "constant.other.color.rgb-value punctuation.definition.constant",
      "settings": {
        "foreground": "<%= orange %>"
      }
    },
    {
      "name": "Constant Other Color",
      "scope": "constant.other.color",
      "settings": {
        "foreground": "<%= brightYellow %>"
      }
    },
    {
      "name": "Keyword Other Unit",
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "<%= brightYellow %>"
      }
    },
    {
      "name": "Meta Selector",
      "scope": "meta.selector",
      "settings": {
        "foreground": "<%= brightMagenta %>",
        "fontStyle": ""
      }
    },
    {
      "name": "Entity Other Attribute Name Id",
      "scope": "entity.other.attribute-name.id",
      "settings": {
        "foreground": "<%= gold %>"
      }
    },
    {
      "name": "Meta Property Name",
      "scope": "meta.property-name",
      "settings": {
        "foreground": "<%= cyan %>"
      }
    },
    {
      "name": "Doctypes",
      "scope": ["entity.name.tag.doctype", "meta.tag.sgml.doctype"],
      "settings": {
        "foreground": "<%= brightMagenta %>",
        "fontStyle": ""
      }
    },
    {
      "name": "Punctuation Definition Parameters",
      "scope": "punctuation.definition.parameters",
      "settings": {
        "foreground": "<%= brightGreen %>"
      }
    },
    {
      "name": "Keyword Control Operator",
      "scope": "keyword.control.operator",
      "settings": {
        "foreground": "<%= brightCyan %>"
      }
    },
    {
      "name": "Keyword Operator Logical",
      "scope": "keyword.operator.logical",
      "settings": {
        "foreground": "<%= brightMagenta %>",
        "fontStyle": ""
      }
    },
    {
      "name": "Variable Instances",
      "scope": [
        "variable.instance",
        "variable.other.instance",
        "variable.readwrite.instance",
        "variable.other.readwrite.instance",
        "variable.other.property"
      ],
      "settings": {
        "foreground": "<%= brightCyan %>"
      }
    },
    {
      "name": "Variable Property Other object property",
      "scope": ["variable.other.object.property"],
      "settings": {
        "foreground": "<%= textLighter %>",
        "fontStyle": ""
      }
    },
    {
      "name": "Variable Property Other object",
      "scope": ["variable.other.object.js"],
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "Entity Name Function",
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "<%= blue %>",
        "fontStyle": ""
      }
    },
    {
      "name": "Keyword Operator Comparison, imports, returns and Keyword Operator Ruby",
      "scope": [
        "keyword.operator.comparison",
        "keyword.control.flow.js",
        "keyword.control.flow.ts",
        "keyword.control.flow.tsx",
        "keyword.control.ruby",
        "keyword.control.module.ruby",
        "keyword.control.class.ruby",
        "keyword.control.def.ruby",
        "keyword.control.loop.js",
        "keyword.control.loop.ts",
        "keyword.control.import.js",
        "keyword.control.import.ts",
        "keyword.control.import.tsx",
        "keyword.control.from.js",
        "keyword.control.from.ts",
        "keyword.control.from.tsx"
      ],
      "settings": {
        "foreground": "<%= brightMagenta %>",
        "fontStyle": ""
      }
    },
    {
      "name": "Keyword Control Conditional",
      "scope": [
        "keyword.control.conditional.js",
        "keyword.control.conditional.ts",
        "keyword.control.switch.js",
        "keyword.control.switch.ts"
      ],
      "settings": {
        "foreground": "<%= brightMagenta %>",
        "fontStyle": ""
      }
    },
    {
      "name": "Support Constant, `new` keyword, Special Method Keyword, `debugger`, other keywords",
      "scope": [
