{
  "name": "outrunner",
  "displayName": "Outrunner",
  "description": "A radical theme for VS Code",
  "version": "0.0.0",
  "publisher": "itsjonq",
  "license": "SEE LICENSE IN LICENSE.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/itsjonq/outrunner"
  },
  "keywords": [
    "Dark",
    "outrun",
    "outrunner",
    "purple",
    "pink",
    "teal",
    "cyan",
    "blue",
    "Theme"
  ],
  "scripts": {
    "build": "npm run clean && node src/index.js",
    "clean": "rm -rf themes",
    "start": "npm run build",
    "git:push": "git push --follow-tags",
    "postversion": "npm run release",
    "release": "vsce publish && npm run git:push"
  },
  "galleryBanner": {
    "color": "#0d0221",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.17.0"
  },
  "categories": [
    "Themes"
  ],
  "icon": "images/refined.png",
  "contributes": {
    "themes": [
      {
        "label": "Outrunner",
        "uiTheme": "vs-dark",
        "path": "./themes/outrunner-theme-outrunner.json"
      }
    ]
  },
  "devDependencies": {
    "glob": "7.1.3",
    "lodash.template": "4.4.0",
    "mkdirp": "0.5.1",
    "polished": "2.3.1"
  }
}